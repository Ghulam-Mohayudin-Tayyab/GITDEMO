@model List<GITDEMO.Models.Student>

<h2>Student List</h2>

 <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Roll No</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.Id</td>
                    <td>@student.Name</td>
                    <td>@student.RollNo</td>
                    <td>@student.Department</td>
                </tr>
            }
        </tbody>
    </table>

