<!-- Views/Home/TeacherList.cshtml -->
@model IEnumerable<GITDEMO.Models.Teacher>

<h2>Teacher List</h2>
<h2>Added by Tayyab 1</h2>
<h2>Added by Tayyab 2</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Subject</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Id</td>
                <td>@teacher.Name</td>
                <td>@teacher.Subject</td>
                <td>@teacher.Salary</td>

            </tr>
        }
    </tbody>
</table>
